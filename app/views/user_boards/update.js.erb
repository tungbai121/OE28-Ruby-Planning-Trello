<% if @board.errors.present? %>
  alert('<%= @board.errors.messages %>');
<% else %>
  $('#star-item').html('<%= j render "boards/starred" %>');
  $('#member-list').html('<%= j render partial: "boards/user", collection: @board.users, as: :user %>');
<% end %>
$('#toastr').html('<%= j render "shared/toastr" %>');
